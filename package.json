{
    "name": "forex-trader-portfolio",
    "version": "1.0.0",
    "description": "Professional Forex Trader Portfolio with Express.js backend",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "client": "cd client && npm run dev",
        "client:build": "cd client && npm run build",
        "client:install": "cd client && npm install",
        "build": "npm run client:build",
        "deploy": "npm run build && NODE_ENV=production node server.js"
    },
    "keywords": [
        "forex",
        "portfolio",
        "express",
        "ejs"
    ],
    "author": "Irshad Sheikh",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "ejs": "^3.1.9",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "multer": "^1.4.5-lts.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1"
    }
}
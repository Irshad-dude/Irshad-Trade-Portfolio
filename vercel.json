{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "client/dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/server"
        },
        {
            "source": "/store/(.*)",
            "destination": "/api/server"
        }
    ],
    "functions": {
        "api/server.js": {
            "memory": 256
        }
    }
}
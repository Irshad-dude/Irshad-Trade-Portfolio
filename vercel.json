{
    "version": 2,
    "functions": {
        "server.js": {
            "runtime": "@vercel/node@3"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server.js"
        },
        {
            "source": "/store/(.*)",
            "destination": "/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/server.js"
        }
    ]
}
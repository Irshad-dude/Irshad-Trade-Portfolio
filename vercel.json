{
    "version": 2,
    "buildCommand": "cd client && npm install && npm run build",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/sitemap.xml",
            "dest": "/sitemap.xml"
        },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/store/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}